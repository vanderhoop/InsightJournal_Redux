<div class="span8">
  <h3>Entry created <%= @day_created %></h3>
  <p><%= @entry.text %></p>
  <%= link_to 'Edit Entry', edit_user_entry_path(@entry), :class => "square-button grey" %>
  <%= link_to 'Delete Entry', @entries, :method => :delete, :confirm => "Are you sure you want to delete this entry?", :class => "square-button grey" %>
</div>
