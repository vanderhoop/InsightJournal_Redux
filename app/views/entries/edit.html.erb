<%= form_for(@entry, :url => { :action => "update", :id => @entry } ) do |f| %>
  <%= f.text_area :text  %>
  <%= f.label :user_mood_input, "Mood at time of writing: " %>
  <%= f.select :user_mood_input, [["1 - Downright Lousy", 1], [ "2", ], ["3",3],["4", 4], ["5 - About Average", 5],["6", 6], ["7", 7], ["8", 8], ["9", 9], ["10        - Outstanding", 10]] %>
  <%= f.submit %>
<% end %>
